<!DOCTYPE html>
<html>
<head>

	{# Metadata #}

	<title>
		{% block PageTitle %}DKE Wiki{% endblock %}
	</title><link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
	<link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png">
	<link rel="manifest" href="/images/manifest.json">
	<link rel="mask-icon" href="/images/safari-pinned-tab.svg" color="#5bbad5">
	<meta name="theme-color" content="#ffffff">

	{% block MetaData %}{% endblock %}

	{# Vendor Styles #}
	<link rel="stylesheet" href="/css/normalize.css" media="screen" charset="utf-8">

	{# Base css #}
	{# Styles for every page #}

	{# Custom #}
	{% block CSS %}{% endblock %}

</head>
<body>

	<div class="container">

		{% block Content %}{% endblock %}

	</div>


	{# Vendor scripts #}

	{# Only get placed on authenticated routes #}
	{% block AuthScripts %}{% endblock %}

	{% block Scripts %}{% endblock %}

</body>
</html>
